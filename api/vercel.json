{
    "version": 2,
    "builds": [
        {
            "src":"*.js", 
            "use": "@vercel/node"
        },
        {
            "src": "client/build/**/*",
            "use": "@vercel/static"
          }
    ],
    "routes":[
        {
            "src": "/(.*)", 
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/build/$1"
        }
    ]
}